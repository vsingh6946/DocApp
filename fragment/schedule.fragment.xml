<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
<TableSelectDialog
       contentHeight="40%"
       contentWidth="40%"
		id="myDialog"
		noDataText="No Products Found"
		title="Available Slots"
		cancel="handleClose"
		items="{bookedSchedule>/}">
		<ColumnListItem id="idColumnListItem" vAlign="Middle">
			<cells>
				
				<Text id="idDate" text="{bookedSchedule>Date}" />
				<Link id="idTime" text="{bookedSchedule>Time}" />
				
			</cells>
		</ColumnListItem>
		<columns>
			<Column id="idColumnDate" width="12em">
				<header>
					<Text id="idDateValue" text="{i18n>Date}" />
				</header>
			</Column>
			<Column id="idColumnTime" >
				<header>
					<Text id="idTimeValue" text="{i18n>Time}" />
				</header>
			</Column>
			
		</columns>
	</TableSelectDialog>
	
</core:FragmentDefinition>