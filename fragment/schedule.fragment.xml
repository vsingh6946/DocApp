<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
<TableSelectDialog
       contentHeight="40%"
       contentWidth="40%"
		id="myDialog"
		noDataText="No Products Found"
		title="Available Slots"
		cancel="handleClose"
		items="{bookedSchedule>/}">
		<ColumnListItem vAlign="Middle">
			<cells>
				
				<Text text="{bookedSchedule>Date}" />
				<Link text="{bookedSchedule>Time}" />
				
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="{i18n>Date}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="{i18n>Time}" />
				</header>
			</Column>
			
		</columns>
	</TableSelectDialog>
	
</core:FragmentDefinition>